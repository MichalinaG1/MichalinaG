using System;

namespace zadanie3
{
    class Program
    {
        static void Main( string[] args)
        {
            //Napisz program drukujący na ekranie kolejne liczby naturalne podzielne przez 7. Ilość liczb
            // wczytaj z klawiatury. Użyj pętli for. Oto wydruk dla ile=6:
            // 0, 7, 14, 21, 28, 35

            int i, ile;
            Console.Write("podaj ile liczb: ");
            ile = Convert.ToInt32(Console.ReadLine());
            for(i=0; i<=(ile*ile);i++)
                if (i % 7 == 0)
                    Console.Write("{0},  ", i);
            Console.ReadKey(true);

        }
    }
}
