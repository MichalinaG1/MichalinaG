using System;

namespace zadanie3
{
    class Program
    {
        static void Main( string[] args)
        {
            // Wczytaj z klawiatury rok_początkowy i rok_końcowy oraz wypisz na ekranie wszystkie lata
            // przestępne od roku_początkowego do roku_końcowego włącznie. Użyj pętli for.

            int i, rok_p, rok_k;
            Console.Write("podaj rok poczatkowy: ");
            rok_p = Convert.ToInt32(Console.ReadLine());
            Console.Write("podaj rok koncowy: ");
            rok_k = Convert.ToInt32(Console.ReadLine());
            
            for (i=rok_p; i<=rok_k; i++)
                // rok przestepny: podzielny przez 4, niepodzielny przez 100, podzielny przez 400
                if (((i%4==0)&&(i%100!=0)) || (i%400==0))
                    Console.Write("{0} ", i);
            Console.ReadKey(true);


        }
    }
}
